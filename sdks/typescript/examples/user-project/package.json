{
  "name": "lilypad-user-example",
  "version": "1.0.0",
  "description": "Example user project using Lilypad TypeScript SDK",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "lilypad-extract && vite build",
    "build:webpack": "lilypad-extract && webpack",
    "extract": "lilypad-extract",
    "extract:watch": "lilypad-extract --watch",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@lilypad/typescript-sdk": "file:../.."
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  }
}
