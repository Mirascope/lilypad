{
  "name": "@lilypad/typescript-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Lilypad - LLM observability and monitoring",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "node": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      },
      "default": "./dist/index.js"
    },
    "./legacy": {
      "types": "./dist/legacy/index.d.ts",
      "require": "./dist/legacy/index.js",
      "default": "./dist/legacy/index.js"
    },
    "./modern": {
      "types": "./dist/modern/index.d.ts",
      "import": "./dist/modern/index.mjs",
      "default": "./dist/modern/index.mjs"
    },
    "./dist/register.js": {
      "require": "./dist/register.js",
      "default": "./dist/register.js"
    },
    "./dist/legacy/register.js": {
      "require": "./dist/legacy/register.js",
      "default": "./dist/legacy/register.js"
    },
    "./dist/modern/register.mjs": {
      "import": "./dist/modern/register.mjs",
      "default": "./dist/modern/register.mjs"
    },
    "./dist/register-esm-loader.mjs": {
      "import": "./dist/register-esm-loader.mjs",
      "default": "./dist/register-esm-loader.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "npm run build:dual && npm run copy-esm-loaders",
    "build:dual": "npm run build:legacy && npm run build:modern && npm run build:default",
    "build:legacy": "tsup --config tsup.legacy.config.ts",
    "build:modern": "tsup --config tsup.modern.config.ts",
    "build:default": "tsup",
    "copy-esm-loaders": "cp src/register-esm-loader.mjs dist/ && cp src/esm-hooks.mjs dist/ && cp src/register-esm-loader.mjs dist/legacy/ && cp src/esm-hooks.mjs dist/legacy/ && cp src/register-esm-loader.mjs dist/modern/ && cp src/esm-hooks.mjs dist/modern/",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit -p tsconfig.typecheck.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix": "eslint . --ext .ts,.tsx --fix && prettier --write .",
    "clean": "rm -rf dist coverage .turbo",
    "prepublishOnly": "bun run build",
    "example:basic": "bun run examples/basic.ts",
    "example:comprehensive": "bun run examples/comprehensive.ts",
    "example:comprehensive:tsx": "tsx --tsconfig tsconfig.tsx.json examples/comprehensive.ts",
    "example:comprehensive:auto": "tsx --require ./dist/register.js examples/comprehensive.ts",
    "example:streaming": "bun run examples/streaming.ts",
    "example:trace-wrap": "bun run examples/trace-wrap-mode.ts",
    "example:versioning": "bun run examples/test-versioning.ts",
    "example:versioning:basic": "bun run examples/versioning-basic.ts",
    "example:versioning:decorator": "bun run examples/versioning-decorator.ts",
    "example:versioning:comparison": "bun run examples/versioning-comparison.ts"
  },
  "keywords": [
    "lilypad",
    "llm",
    "observability",
    "monitoring",
    "opentelemetry",
    "tracing",
    "openai"
  ],
  "author": "Mirascope",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mirascope/lilypad.git",
    "directory": "sdks/typescript"
  },
  "bugs": {
    "url": "https://github.com/Mirascope/lilypad/issues"
  },
  "homepage": "https://github.com/Mirascope/lilypad#readme",
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/instrumentation": "^0.203.0",
    "@opentelemetry/resources": "^1.19.0",
    "@opentelemetry/sdk-node": "^0.46.0",
    "@opentelemetry/sdk-trace-base": "^1.19.0",
    "@opentelemetry/sdk-trace-node": "^1.19.0",
    "@opentelemetry/semantic-conventions": "^1.34.0",
    "@types/shimmer": "^1.2.0",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "import-in-the-middle": "^1.14.2",
    "js-base64": "^3.7.7",
    "qs": "^6.14.0",
    "shimmer": "^1.2.1"
  },
  "peerDependencies": {
    "openai": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.12.11",
    "@types/node": "^20.19.4",
    "@types/qs": "^6.14.0",
    "@types/readable-stream": "^4.0.21",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "openai": "^4.0.0",
    "prettier": "^3.0.0",
    "readable-stream": "^4.7.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
